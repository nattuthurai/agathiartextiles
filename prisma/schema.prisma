generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:Welcome123@localhost:3306/agtext"
}

model Yarns {
  id      Int    @id @default(autoincrement())
  name    String @db.VarChar(50)
  address String @db.VarChar(50)
  date    String @db.VarChar(50)
}

model Colors {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(50)
  date String @db.VarChar(50)
}
